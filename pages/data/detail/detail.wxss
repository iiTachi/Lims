.section {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-left: 15px;
  margin-right: 15px;
  border-bottom: 1px solid rgb(240, 240, 240);
  height: 60px;
}

.describe {
  font-size: 20px;
  color: rgb(120, 120, 120);
}

.data {
  font-size: 20px;
  color: #4169E1;
  position: absolute;
  right: 15px;
}

.maps {
  width: 60px;
  height: 60px;
  position: absolute;
  right: 0;
}